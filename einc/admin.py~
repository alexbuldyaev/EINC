from django.contrib import admin
from .models import *

class showname (admin.ModelAdmin):
    sname = ('name')


admin.site.register(InsComp, showname)
admin.site.register(CalcRul)
admin.site.register(Insured, showname)
admin.site.register(Owner, showname)
admin.site.register(Driver, showname)
admin.site.register(Usercli)
admin.site.register(PolicyType, showname)
admin.site.register(Status, showname)
admin.site.register(PolicyCar)
admin.site.register(PolicyHome)
admin.site.register(Order)


