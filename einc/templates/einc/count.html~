	koeff['Мощность']=[]
	
	if((form.elements['Тип'].value=='легковое') || (form.elements['Тип'].value=='такси')){
		koeff['Мощность']['<=50']=0.6;
		koeff['Мощность']['50-70']=1;
		koeff['Мощность']['70-100']=1.1;
		koeff['Мощность']['100-120']=1.2;
		koeff['Мощность']['120-150']=1.4;
		koeff['Мощность']['>150']=1.6;
	}
	else {
		koeff['Мощность']['<=50']=1;
		koeff['Мощность']['50-70']=1;
		koeff['Мощность']['70-100']=1;
		koeff['Мощность']['100-120']=1;
		koeff['Мощность']['120-150']=1;
		koeff['Мощность']['>150']=1;
	}

	koeff['Список допущенных водителей']=[]

	if (form.elements['Собственник'].options[form.elements['Собственник'].selectedIndex].value=='юридическое') {
		
		sum*=1.8
		sum2*=1.8
		
	}
	else {
		koeff['Список допущенных водителей']['ограниченный']=1
		koeff['Список допущенных водителей']['неограниченный']=1.8
		if(form.elements['Список допущенных водителей'].value=='ограниченный'){
			if(form.elements['Минимальный возраст'].value=='до 22 лет включительно'){
				if(form.elements['Минимальный стаж'].value=='до 3 лет включительно'){
					sum*=1.8
					sum2*=1.8
				}else{
					sum*=1.6
					sum2*=1.6
				}
			}else{
				if(form.elements['Минимальный стаж'].value=='до 3 лет включительно'){
					sum*=1.7
					sum2*=1.7
				}else{
