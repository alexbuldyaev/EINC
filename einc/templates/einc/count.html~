<script>
var fields=[];
fields['1']='Собственник';
fields['2']='Тип';
fields['3']='Место регистрации';
fields['4']='Мощность';
fields['5']='Список допущенных водителей';
fields['6']='Минимальный возраст';
fields['7']='Минимальный стаж';
fields['8']='Период использования';
fields['9']='Лет безаварийной езды';

var finish=false;
var current_ceil=0;



function re_count(){
	form=document.getElementById('mainform')

	var sum=0;
	var sum2=0;

	if(form.elements['Тип'].value=='1'){
		if(form.elements['Собственник'].value=='1'){
			if(form.elements['Мощность'].value=='100-120' || form.elements['Мощность'].value=='120-150' || form.elements['Мощность'].value=='>150'){
				sum=3432
				sum2=4118
			}else{
				sum=4118
			}	
		}else{
			sum=2573
		}
	}else if(form.elements['Тип'].value=='2'){
		sum=6166
	}else if(form.elements['Тип'].value=='3'){
		sum=3509
	}else if(form.elements['Тип'].value=='4'){
		sum=5284
	}else if(form.elements['Тип'].value=='5'){
		sum=867
	}
	
	koeff=[];
	koeff['Место регистрации']=[]
	koeff['Место регистрации']['1']=2;
	koeff['Место регистрации']['2']=1.7;
	koeff['Место регистрации']['3']=1.8;
	koeff['Место регистрации']['4']=1.6;
	koeff['Место регистрации']['5']=1.7;
	
	koeff['Мощность']=[];	
	if((form.elements['Тип'].value=='1') || (form.elements['Тип'].value=='2')){
		koeff['Мощность']['1']=0.6;
		koeff['Мощность']['2']=1;
		koeff['Мощность']['3']=1.1;
		koeff['Мощность']['4']=1.2;
		koeff['Мощность']['5']=1.4;
		koeff['Мощность']['6']=1.6;
	}
	else {
		koeff['Мощность']['1']=1;
		koeff['Мощность']['2']=1;
		koeff['Мощность']['3']=1;
		koeff['Мощность']['4']=1;
		koeff['Мощность']['5']=1;
		koeff['Мощность']['6']=1;
	}

	koeff['Список допущенных водителей']=[]

	if (form.elements['Собственник'].options[form.elements['Собственник'].selectedIndex].value=='2') {
		
		sum*=1.8
		sum2*=1.8

	}
	else {
		koeff['Список допущенных водителей']['1']=1
		koeff['Список допущенных водителей']['2']=1.8
		if(form.elements['Список допущенных водителей'].value=='2'){
			if(form.elements['Минимальный возраст'].value=='1'){
				if(form.elements['Минимальный стаж'].value=='1'){
					sum*=1.8
					sum2*=1.8
				}else{
					sum*=1.6
					sum2*=1.6
				}
			}else{
				if(form.elements['Минимальный стаж'].value=='1'){
					sum*=1.7
					sum2*=1.7
				}else{
					sum*=1
					sum2*=1
				}
			}
		}
	}
		
	koeff['Период использования']=[]
	koeff['Период использования']['1']=0.5
	koeff['Период использования']['2']=0.6
	koeff['Период использования']['3']=0.65
	koeff['Период использования']['4']=0.7
	koeff['Период использования']['5']=0.8
	koeff['Период использования']['6']=0.9
	koeff['Период использования']['7']=0.95
	koeff['Период использования']['8']=1

	
	koeff['Лет безаварийной езды']=[]
	koeff['Лет безаварийной езды']['1']=1
	koeff['Лет безаварийной езды']['2']=0.95
	koeff['Лет безаварийной езды']['3']=0.9
	koeff['Лет безаварийной езды']['4']=0.85
	koeff['Лет безаварийной езды']['5']=0.8
	koeff['Лет безаварийной езды']['6']=0.75
	koeff['Лет безаварийной езды']['7']=0.7
	koeff['Лет безаварийной езды']['8']=0.65
	koeff['Лет безаварийной езды']['9']=0.6
	koeff['Лет безаварийной езды']['10']=0.55
	koeff['Лет безаварийной езды']['11']=0.5

	msg=''
	for(var i=3;i<=9;i++){
		if(koeff[fields[''+i]] && koeff[fields[''+i]][form.elements[fields[''+i]].value]){
		
			if((fields[''+i]=='Минимальный возраст' || fields[''+i]=='Минимальный стаж') && form.elements['Список допущенных водителей'].value=='2') continue;

			sum = sum * koeff[fields[''+i]][form.elements[fields[''+i]].value]
			sum2 = sum2 * koeff[fields[''+i]][form.elements[fields[''+i]].value]
		}
	}
	
	if(sum2==0){
		document.getElementById('result').innerHTML=Math.round((sum*100)/100)
                document.getElementById('result1').innerHTML=Math.round((sum*100)/100)
                document.getElementById('result2').innerHTML=Math.round((sum*100)/100)
                document.getElementById('result3').innerHTML=Math.round((sum*115)/100)
                document.getElementById('result4').innerHTML=Math.round((sum*101)/100)
                document.getElementById('result5').innerHTML=Math.round((sum*99)/100)
                document.getElementById('result6').innerHTML=Math.round((sum*109)/100)
	}else{
		document.getElementById('result').innerHTML=(Math.round((sum*100)/100))
                document.getElementById('result1').innerHTML=(Math.round((sum*100)/100))
                document.getElementById('result2').innerHTML=(Math.round((sum*100)/100))
                document.getElementById('result3').innerHTML=(Math.round((sum*115)/100))
                document.getElementById('result4').innerHTML=(Math.round((sum*101)/100))
                document.getElementById('result5').innerHTML=(Math.round((sum*99)/100))
                document.getElementById('result6').innerHTML=(Math.round((sum*109)/100))
	}
        var elem=document.getElementById('result');

	unactivetocar = [];
	for(var i=1;i<=9;i++){
		unactivetocar[i] = false;
	}
	

	if (form.elements[fields['1']].selectedIndex == 2) {
		unactivetocar[5] = true;
		form.elements[fields['5']].parentNode.style.display = 'none';
		form.elements['Список допущенных водителей_neogr'].style.display = '';
		
		if (form.elements[fields['5']].selectedIndex == 2) {
			unactivetocar[6] = unactivetocar[7] = true;
			form.elements[fields['6']].parentNode.style.display = 'none';
			form.elements['Минимальный возраст_neogr'].style.display = '';
			form.elements[fields['7']].parentNode.style.display = 'none';
			form.elements['Минимальный стаж_neogr'].style.display = '';
		}
		else {
			unactivetocar[6] = unactivetocar[7] = true;
			form.elements[fields['6']].parentNode.style.display = 'none';
			form.elements['Минимальный возраст_neogr'].style.display = '';
			form.elements[fields['7']].parentNode.style.display = 'none';
			form.elements['Минимальный стаж_neogr'].style.display = '';
		}
	}
	else {
		unactivetocar[5] = false;
		form.elements[fields['5']].parentNode.style.display = '';
		form.elements['Список допущенных водителей_neogr'].style.display = 'none';
		
		if (form.elements[fields['5']].selectedIndex == 2) {
			unactivetocar[6] = unactivetocar[7] = true;
			form.elements[fields['6']].parentNode.style.display = 'none';
			form.elements['Минимальный возраст_neogr'].style.display = '';
			form.elements[fields['7']].parentNode.style.display = 'none';
			form.elements['Минимальный стаж_neogr'].style.display = '';
		}
		else {
			unactivetocar[6] = unactivetocar[7] = false;
			form.elements[fields['6']].parentNode.style.display = '';
			form.elements['Минимальный возраст_neogr'].style.display = 'none';
			form.elements[fields['7']].parentNode.style.display = '';
			form.elements['Минимальный стаж_neogr'].style.display = 'none';
		}
	}

}

setInterval('re_count()', 30)
</script>



