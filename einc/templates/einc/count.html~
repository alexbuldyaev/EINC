	for(var i=3;i<=9;i++){
		if(koeff[fields[''+i]] && koeff[fields[''+i]][form.elements[fields[''+i]].value]){
		
			if((fields[''+i]=='Минимальный возраст' || fields[''+i]=='Минимальный стаж') && form.elements['Список допущенных водителей'].value=='неограниченный') continue;

			sum = sum * koeff[fields[''+i]][form.elements[fields[''+i]].value]
			sum2 = sum2 * koeff[fields[''+i]][form.elements[fields[''+i]].value]
		}
	}
	
	if(sum2==0){
		document.getElementById('result').innerHTML=Math.round(sum*100)/100
	}else{
		document.getElementById('result').innerHTML=(Math.round(sum*100)/100)+' &minus; '+(Math.round(sum2*100)/100)
	}
	

	if (form.elements[fields['1']].selectedIndex == 2) {
		unactivetocar[5] = true;
		form.elements[fields['5']].parentNode.style.display = 'none';
		form.elements['Список допущенных водителей_neogr'].style.display = '';
		
		if (form.elements[fields['5']].selectedIndex == 2) {
			unactivetocar[6] = unactivetocar[7] = true;
			form.elements[fields['6']].parentNode.style.display = 'none';
			form.elements['Минимальный возраст_neogr'].style.display = '';
			form.elements[fields['7']].parentNode.style.display = 'none';
			form.elements['Минимальный стаж_neogr'].style.display = '';
		}
		else {
			unactivetocar[6] = unactivetocar[7] = true;
			form.elements[fields['6']].parentNode.style.display = 'none';
			form.elements['Минимальный возраст_neogr'].style.display = '';
			form.elements[fields['7']].parentNode.style.display = 'none';
			form.elements['Минимальный стаж_neogr'].style.display = '';
		}
	}
	else {
		unactivetocar[5] = false;
		form.elements[fields['5']].parentNode.style.display = '';
		form.elements['Список допущенных водителей_neogr'].style.display = 'none';
		
		if (form.elements[fields['5']].selectedIndex == 2) {
			unactivetocar[6] = unactivetocar[7] = true;
			form.elements[fields['6']].parentNode.style.display = 'none';
			form.elements['Минимальный возраст_neogr'].style.display = '';
			form.elements[fields['7']].parentNode.style.display = 'none';
			form.elements['Минимальный стаж_neogr'].style.display = '';
		}
		else {
			unactivetocar[6] = unactivetocar[7] = false;
			form.elements[fields['6']].parentNode.style.display = '';
			form.elements['Минимальный возраст_neogr'].style.display = 'none';
			form.elements[fields['7']].parentNode.style.display = '';
			form.elements['Минимальный стаж_neogr'].style.display = 'none';
		}
	}

}

